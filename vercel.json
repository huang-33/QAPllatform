{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.8"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "app.py",
      "methods": ["GET", "POST"]
    }
  ]
}
